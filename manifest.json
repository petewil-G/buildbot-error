{
  "name": "BuildBot Error 2",
  "version": "15",
  "description": "Skip through errors and failures in buildbot output.",
  "content_scripts": [
    {
      "matches": [
        "*://logs.chromium.org/*",
        "*://luci-logdog.appspot.com/*"
      ],
      "js": ["nexterror.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "*://logs.chromium.org/*",
    "*://luci-logdog.appspot.com/*"
  ],
  "manifest_version": 2
}
